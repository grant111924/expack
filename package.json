{
  "name": "expack",
  "version": "1.0.0",
  "description": "Expack is the bare-bones Express and Webpack boilerplate with ES6+ babel transpilation, ESLint linting, Hot Module Reloading, and Jest test framework enabled.",
  "scripts": {
    "buildDev": "rimraf dist && webpack --mode development --config webpack.server.config.js && cross-env NODE_ENV=development webpack --config webpack.client.config.js",
    "buildProd": "rimraf dist && webpack --mode production --config webpack.server.config.js && cross-env NODE_ENV=production webpack --config webpack.client.config.js",
    "start": "node ./dist/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.0",
    "bootstrap4-toggle": "^3.6.1",
    "express": "^4.17.1",
    "lowdb": "^1.0.0",
    "pug": "^2.0.4"
  },
  "engines": {
    "node": ">=4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "autoprefixer": "^9.7.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.6",
    "babel-eslint": "^9.0.0",
    "chokidar": "^3.3.1",
    "css-loader": "^3.2.0",
    "cross-env": "^7.0.0",
    "eslint": "^4.19.1",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "pug-html-loader": "^1.1.5",
    "postcss-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.4.1",
    "mini-css-extract-plugin": "^0.4.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "rimraf": "^3.0.0",
    "raw-loader": "^4.0.0",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2",
    "webpack-dev-server": "^3.7.2"
  }
}
